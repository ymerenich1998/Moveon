<p id="notice"><%= notice %></p>

<h1>Vehicle Marks</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @vehicle_marks.each do |vehicle_mark| %>
      <tr>
        <td><%= vehicle_mark.name %></td>
        <td><%= link_to 'Show', vehicle_mark %></td>
        <td><%= link_to 'Edit', edit_vehicle_mark_path(vehicle_mark) %></td>
        <td><%= link_to 'Destroy', vehicle_mark, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Vehicle Mark', new_vehicle_mark_path %>
